  "bugs": {
    "url": "https://github.com/mcollina/aedes-stats/issues"
  },
  "homepage": "https://github.com/mcollina/aedes-stats#readme",
  "devDependencies": {
    "aedes": "^0.39.0",
    "faucet": "0.0.1",
    "license-checker": "^25.0.1",
    "mqtt": "^3.0.0",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "qlobber": "^3.1.0",
    "snazzy": "^8.0.0",
    "standard": "^13.1.0",
    "tape": "^4.11.0"
  },
  "dependencies": {}
